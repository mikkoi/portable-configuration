[core]
    autocrlf = input
    safecrlf = true
    editor = vim
    excludesfile = ~/.gitignore_global
[alias]
    co = checkout
    st = status
    br = branch --all --verbose --verbose
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    re = remote --verbose --verbose
    type = cat-file -t
    dump = cat-file -p
    last = log -n1 --format=raw
